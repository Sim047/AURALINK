{
  "expo": {
    "name": "Auralink Expo App",
    "slug": "auralink-expo-app",
    "scheme": "auralink",
    "version": "0.1.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.auralink.app"
    },
    "android": {
      "package": "com.auralink.app",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "extra": {
      "apiUrl": "https://auralink-app-2u3p.vercel.app/api",
      "apiBase": "https://auralink-app-2u3p.vercel.app",
      "webUrl": "https://auralink-app-2u3p.vercel.app",
      "eas": {
        "projectId": "662fee92-4be6-4230-8e4f-c23bde091223"
      }
    },
    "plugins": [
      "expo-secure-store"
    ],
    "owner": "simsweb"
  }
}
